modules = ["nodejs-20"]
[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd server && npm start"
waitForPort = 3001

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 3001
externalPort = 80

[deployment]
run = ["sh", "-c", "cd server && npm start"]
deploymentTarget = "cloudrun"
