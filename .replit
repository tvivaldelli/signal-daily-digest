modules = ["nodejs-20"]
[agent]
expertMode = true

[workflows]

[[workflows.workflow]]
name = "Project"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd server && node index.js"
waitForPort = 3001

[[ports]]
localPort = 3001
externalPort = 3001

[deployment]
run = ["sh", "-c", "cd server && node index.js"]
deploymentTarget = "cloudrun"

[nix]
channel = "stable-25_05"
